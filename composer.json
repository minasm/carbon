{
    "name": "minasm/carbon-uk",
    "description": "A basic wrapper for citco/carbon which returns business days between two given dates.",
    "keywords": ["british","england","wales","bank","holiday","holidays","business days"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.3",
        "citco/carbon": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Minasm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Minasm\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4.0",
        "laravel/pint": "^v1.18",
        "phpstan/phpstan": "^1.12"
      }
}
